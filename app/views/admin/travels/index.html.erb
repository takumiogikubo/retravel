<table class="table">
  <tr>
    <th class="text-center">作成日時</th>
    <th class="text-center">アカウント名</th>
    <th class="text-center">投稿画像</th>
    <th class="text-center">タイトル</th>
    <th class="text-center">ステータス</th>
  </tr>
    <% @travels.reverse.each do |travel| %>
  <tr>
    <td class="align-middle text-center" style="width: 20%"><%= travel.created_at.strftime('%Y年%m月%d日 %H:%M:%S')%></td>
    <td class="align-middle text-center"><%= travel.customer.account%></td>
    <td class="align-middle text-center">
      <%= image_tag travel.get_image(70,70)%>
    </td>
    <td class="align-middle text-center">
      <%=link_to admin_travel_path(travel) do%>
        <%= travel.travel_title%>
      <%end%>
    </td>
    <td class="align-middle text-center">
      <% if travel.open %>
        <p class="text-warning mb-0"><strong>公開</strong></p>
      <% else %>
        <p class="text-secondary mb-0"><strong>非公開</strong></p>
      <% end %>
    </td>
  </tr>
    <%end%>
</table>